import cv2
import numpy

place1 = cv2.imread('place1.jpg')


place1.shape

place2 = cv2.imread('place2.jpg')

place2.shape

cv2.imshow("place1",place1)
cv2.waitKey()
cv2.destroyAllWindows()
cv2.imshow("place2",place2)
cv2.waitKey()
cv2.destroyAllWindows()


c_image = cv2.hconcat([place1, place2])

cv2.imshow("Collage", c_image)
cv2.waitKey()
cv2.destroyAllWindows()
